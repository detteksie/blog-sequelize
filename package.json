{
  "name": "blog-sequelize",
  "version": "1.0.0",
  "description": "Blog CRUD with Sequelize TS",
  "author": "Sulthon Abdul Malik <sulthon.abdmalik@gmail.com>",
  "license": "MIT",
  "main": "build/bin/www",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc -b",
    "start": "node .",
    "start:dev": "cross-env NODE_ENV=development env-cmd ts-node-dev --respawn --transpile-only src/bin/www",
    "start:debug": "cross-env DEBUG=blog-sequelize:* npm run start:dev",
    "prepare": "husky install",
    "prettify": "prettier -w \"{config,db,public,src}/**/*.{md,html,css,json,js,ts}\""
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pug": "^3.0.2",
    "sequelize": "^6.15.1",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.15",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{md,html,css,json,js,ts}": "prettier -w"
  }
}
